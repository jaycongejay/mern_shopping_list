{
  "name": "mern_shopping_list",
  "version": "1.0.0",
  "description": "Shopping list with MERN stack",
  "main": "server.js",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaycongejay/mern_shopping_list.git"
  },
  "author": "Jay L",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaycongejay/mern_shopping_list/issues"
  },
  "homepage": "https://github.com/jaycongejay/mern_shopping_list#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
